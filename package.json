{
  "name": "collection-admin",
  "version": "0.4.0",
  "description": "Client-side React-based CRUD admin",
  "author": "Taurus Colvin <taurus.colvin@gmail.com> (http://tauruscolvin.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colvint/collection-admin.git"
  },
  "main": "./dist/collection-admin.js",
  "scripts": {
    "test": "jest",
    "test:watch": "watch 'jest' tests src",
    "build": "BABEL_ENV=production babel src --out-dir dist",
    "prepublish": "npm run build",
    "watch": "npm run watch:server & npm run watch:bundle & wait",
    "watch:server": "nodemon --exec babel-node --ignore static/bundle.js -- static/server.js",
    "watch:bundle": "watchify static/client.js -v -t babelify -g envify -p livereactload -o static/bundle.js"
  },
  "keywords": [
    "meteor",
    "react",
    "client",
    "admin"
  ],
  "jest": {
    "testDirectoryName": "tests",
    "unmockedModulePathPatterns": [
      "./test-fixtures",
      "./node_modules/classnames",
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/react-addons-update",
      "./node_modules/chance",
      "./node_modules/underscore",
      "./node_modules/sinon"
    ]
  },
  "dependencies": {
    "chance": "^1.0.2",
    "classnames": "^2.2.3",
    "react": "^15.0.1",
    "react-addons-update": "^15.0.1",
    "react-bootstrap": "^0.29.0",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-jest": "^11.0.1",
    "babel-plugin-react-transform": "^2.0.2",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "babelify": "^7.2.0",
    "browserify": "^13.0.0",
    "envify": "^3.4.0",
    "express": "^4.13.4",
    "jest-cli": "^11.0.1",
    "livereactload": "^2.2.2",
    "nodemon": "^1.9.1",
    "react-addons-test-utils": "^15.0.1",
    "react-proxy": "^1.1.8",
    "sinon": "^1.17.3",
    "uglifyify": "^3.0.1",
    "watch": "^0.17.1",
    "watchify": "^3.7.0"
  }
}
