{
  "name": "collection-admin",
  "version": "0.1.0",
  "description": "Client-side React-based CRUD admin",
  "author": "Taurus Colvin <taurus.colvin@gmail.com> (http://tauruscolvin.com)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/colvint/collection-admin.git"
  },
  "main": "./dist/collection-admin.js",
  "scripts": {
    "test": "jest",
    "build": "babel src --out-dir dist",
    "test:watch": "watch 'jest' tests src",
    "build:watch": "watch 'npm run build' src",
    "prepublish": "npm run build"
  },
  "keywords": [
    "meteor",
    "react",
    "client",
    "admin"
  ],
  "jest": {
    "testDirectoryName": "tests",
    "unmockedModulePathPatterns": [
      "./node_modules/react",
      "./node_modules/react-dom",
      "./node_modules/react-addons-test-utils",
      "./node_modules/chance",
      "./node_modules/underscore",
      "./node_modules/sinon"
    ]
  },
  "dependencies": {
    "chance": "^1.0.2",
    "react": "^15.0.1",
    "react-bootstrap": "^0.29.0",
    "react-dom": "^15.0.1",
    "react-router": "^2.0.1",
    "underscore": "^1.8.3",
    "underscore.string": "^3.3.4"
  },
  "devDependencies": {
    "babel-cli": "^6.7.5",
    "babel-jest": "^11.0.1",
    "babel-polyfill": "^6.7.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-react": "^6.5.0",
    "jest-cli": "^11.0.1",
    "react-addons-test-utils": "^15.0.1",
    "sinon": "^1.17.3",
    "watch": "^0.17.1"
  }
}
